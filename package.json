    "@radix-ui/react-tooltip": "^1.2.7",
    "@supabase/supabase-js": "^2.86.0",
    "@tanstack/react-query": "^5.83.0",
    "axios": "^1.13.2",
    "baseline-browser-mapping": "^2.9.11",
    "caniuse-lite": "^1.0.30001762",
    "class-variance-authority": "^0.7.1",
